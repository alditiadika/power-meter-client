{
  "name": "power-meter-client",
  "version": "1.0.0",
  "description": "Power Meter Dashboard client using React, Nivo, and Webpack",
  "main": "express-start.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --hot --progress --open --port 4005",
    "local:start": "npm run remove:config && npm run copy:config:local && npm run lint && webpack-dev-server --mode development --hot --progress --open --port 4004",
    "dev:start": "npm run remove:config && npm run copy:config:dev && npm run lint && webpack-dev-server --mode development --hot --progress --open --port 4005",
    "uat:start": "npm run remove:config && npm run copy:config:uat && npm run lint && webpack-dev-server --mode development --hot --progress --open --port 4006",
    "build": "rm -rf dist && webpack --mode production",
    "lint": "eslint --fix ./src",
    "build:start": "node express-start.js",
    "simulate:prod": "rm -rf dist && webpack --mode production && node express-start.js",
    "remove:config": "rm -rf config/config.json",
    "copy:config:dev": "cp -f src/config/config-dev.json src/config/config.json",
    "copy:config:dev-site": "cp -f src/config/config-dev-site.json src/config/config.json",
    "copy:config:uat": "cp -f src/config/config-uat.json src/config/config.json",
    "copy:config:local": "cp -f src/config/config-local.json src/config/config.json",
    "clean": "rm -rf node-modules && rm -rf package-lock.json && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alditiadika/power-meter-client.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alditiadika/power-meter-client/issues"
  },
  "homepage": "https://github.com/alditiadika/power-meter-client#readme",
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "@nivo/core": "^0.67.0",
    "@nivo/generators": "^0.67.0",
    "@nivo/line": "^0.67.0",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "bootstrap": "^4.4.1",
    "brotli-webpack-plugin": "^1.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "d3-time": "^2.0.0",
    "d3-time-format": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "express": "^4.17.1",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.15",
    "lodash-joins": "^2.0.3",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.24.0",
    "node-gyp": "^6.1.0",
    "node-zopfli-es": "^1.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prop-types": "^15.7.2",
    "query-string": "^6.11.1",
    "react": "^16.13.0",
    "react-apollo": "^3.1.3",
    "react-datepicker": "^3.0.0",
    "react-dom": "^16.13.0",
    "react-lazy-load": "^3.0.13",
    "react-loadable": "^5.5.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "shrink-ray-current": "^4.1.2",
    "style-loader": "^1.1.3",
    "svg-url-loader": "^4.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "workbox-webpack-plugin": "^5.1.2"
  }
}
